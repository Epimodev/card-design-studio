{
  "name": "card-design-studio",
  "description": "Card configurator with 3D preview",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": "Frederic Godin <godin.fred8@gmail.com>",
  "contributors": [
    "Mathieu Acthernoene <mathieu.acthernoene@swan.io>"
  ],
  "homepage": "https://github.com/swan-io/card-design-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/swan-io/card-design-studio.git"
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .hooks",
    "tscss": "tcm --pattern ./src/**/*.module.css",
    "tscheck": "tsc --noEmit",
    "compress": "find ./src/dist \\( -name '*.html' -o -name '*.css' -o -name '*.js' -o -name '*.json' -o -name '*.svg' -o -name '*.gltf' \\) -exec gzip -k -9 {} \\; -exec brotli --quality=11 {} \\;",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview"
  },
  "dependencies": {
    "@react-aria/button": "^3.3.1",
    "@react-aria/focus": "^3.2.4",
    "@react-aria/interactions": "^3.4.0",
    "@react-aria/progress": "^3.1.1",
    "@react-aria/radio": "^3.1.3",
    "@react-aria/slider": "^3.0.1",
    "@react-aria/textfield": "^3.2.2",
    "@react-aria/utils": "^3.8.0",
    "@react-aria/visually-hidden": "^3.2.1",
    "base-64": "^1.0.0",
    "clsx": "^1.1.1",
    "dat.gui": "^0.7.7",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-dropzone": "^11.3.2",
    "react-spring": "^9.1.2",
    "resize-observer-polyfill": "^1.5.1",
    "three": "^0.127.0",
    "ts-pattern": "^3.1.2"
  },
  "devDependencies": {
    "@types/base-64": "^0.1.3",
    "@types/dat.gui": "^0.7.6",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/three": "^0.127.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "typed-css-modules": "^0.6.8",
    "typescript": "^4.1.3",
    "vite": "^2.1.0"
  }
}
