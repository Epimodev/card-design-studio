{
  "name": "@swan-io/card-design-studio",
  "description": "Card configurator with 3D preview",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "yarn": "^1.22.0"
  },
  "author": "Frederic Godin <godin.fred8@gmail.com>",
  "homepage": "https://github.com/swan-io/card-design-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/swan-io/card-design-studio.git"
  },
  "scripts": {
    "prepare": "git config --local core.hooksPath .hooks",
    "format": "prettier '**/*' --ignore-unknown --write",
    "link-locale-lake": "sh scripts/lake/link-locale-lake.sh",
    "unlink-locale-lake": "sh scripts/lake/unlink-locale-lake.sh",
    "typecheck": "tsc --noEmit",
    "dev": "vite --port 3000",
    "build": "vite build",
    "serve": "vite preview"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "src/**/*.{js,ts,tsx}": "eslint --fix"
  },
  "browserslist": [
    ">0.2%",
    "not op_mini all",
    "not dead",
    "not ie <= 11",
    "safari >= 12"
  ],
  "dependencies": {
    "@formatjs/intl": "2.9.0",
    "@swan-io/boxed": "1.0.2",
    "@swan-io/chicane": "1.4.1",
    "@swan-io/lake": "2.7.5",
    "core-js": "3.31.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native-web": "0.19.7",
    "ts-pattern": "5.0.4"
  },
  "devDependencies": {
    "@types/node": "20.4.8",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@types/react-native": "0.72.2",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "@vitejs/plugin-legacy": "4.1.1",
    "@vitejs/plugin-react-swc": "3.3.2",
    "dotenv": "16.3.1",
    "eslint": "8.45.0",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-native": "4.0.0",
    "eslint-plugin-swan": "file:scripts/eslint",
    "lint-staged": "13.2.3",
    "pathe": "1.1.1",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.3",
    "type-fest": "4.0.0",
    "typescript": "5.1.6",
    "valienv": "0.5.0",
    "vite": "4.4.6"
  }
}
